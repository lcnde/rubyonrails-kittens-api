
<%= form_with model: @kitten do |f| %>
  <%= f.label :name, 'Name: ' %>
  <%= f.text_field :name %> |
  <%= f.label :age, 'Age: ' %>
  <%= f.number_field :age %> |
  <%= f.label :cuteness, 'Cuteness: ' %>
  <%= f.number_field :cuteness %> |
  <%= f.label :softness, 'Softness: '%>
  <%= f.number_field :softness %><br><br>
  <%= f.submit %>
<% end %>
